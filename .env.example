# Telegram
BOT_TOKEN=123456:ABC-DEF...

# PostgreSQL
DATABASE_URL=postgresql+asyncpg://bot:bot@postgres:5432/bot

# Redis (FSM storage)
REDIS_URL=redis://redis:6379/0

# Ubilling API
UBILLING_URL=http://demo.ubilling.net.ua:9999/billing/userstats

# опционально, MD5 серийного номера (extended auth)
UBILLING_UBER_KEY=

# Настройки
SESSION_TTL_HOURS=-1             # время жизни сессии авторизации
DEFAULT_LOCALE=uk                # локаль по умолчанию (en, uk, ru)
LOG_LEVEL=DEBUG                  # уровень логирования
